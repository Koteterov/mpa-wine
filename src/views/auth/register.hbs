<main>
    <h1>Register Form</h1>
    <div class="form">
        <form method="POST">
            <label for="username">Email</label>
            <input type="text" id="username" name="username" value="{{userData.username}}">

            <label for="password">Password</label>
            <input type="password" id="password" name="password" value="{{userData.password}}">

            <label for="repeatPassword">Re-Password</label>
            <input type="password" id="repeatPassword" name="repeatPassword" value="{{userData.repeatPassword}}">
            <input type="submit" value="Register">
        </form>
    </div>
</main>