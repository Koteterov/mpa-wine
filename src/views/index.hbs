<main>
    <h1>Browser</h1>
    <form action="" method="">
        <input type="text" class="search" name="search" placeholder="Search..." value={{search}}>
        <input type="number" name="from" class="difficulty" placeholder="Market rating from..." value={{from}}>
        <span>-</span>
        <input type="number" name="to" class="difficulty" placeholder="Market rating to..." value={{to}}>
        <input type="submit" value="search">
    </form>

    {{#each wines}}
    {{> wine}}
    {{/each}}

</main>